# Token del bot de Discord (REQUERIDO)
# Obtén tu token en: https://discord.com/developers/applications
DISCORD_TOKEN=tu_token_aqui

# ID del servidor de Discord (Opcional)
GUILD_ID=

# ID del canal de bienvenida (Opcional)
WELCOME_CHANNEL_ID=

# ID del canal de logs de moderación (Opcional)
LOG_CHANNEL_ID=

# Prefijo de comandos (Opcional, default: /)
PREFIX=/

# Token de GitHub para comandos de estado (Opcional)
# Necesario para comandos /github_prs y /github_deployments
GITHUB_TOKEN=

# Configuración del repositorio de GitHub (Opcional)
# Por defecto: R-afk2550/discord-bot-gaming
GITHUB_REPO_OWNER=R-afk2550
GITHUB_REPO_NAME=discord-bot-gaming
